-form_for (:input_file, :url => {:action => 'create'}, :html => { :multipart => true, :id=> 'upload_form' }) do |f|
  = f.file_field :bytes, :onchange => 'submit(event)'
  
:javascript

  function submit(event){
    $(event.element()).up("form").submit();
  }
  
  var result = #{@upload_result};
  window.parent.file_upload(result);
  
